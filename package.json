{
  "name": "@thisismissem/adonisjs-atproto-oauth",
  "description": "Adonis.js provider for doing AT Protocol OAuth logins",
  "version": "1.0.1",
  "engines": {
    "node": ">=20.6.0"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48",
  "type": "module",
  "files": [
    "build/src",
    "build/providers",
    "build/services",
    "build/stubs",
    "build/index.d.ts",
    "build/index.js",
    "build/configure.d.ts",
    "build/configure.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./provider": "./build/providers/oauth_provider.js",
    "./types": "./build/src/types.js",
    "./auth/provider": "./build/src/atproto_auth_provider.js",
    "./initialize_atproto_auth_middleware": "./build/src/middleware.js"
  },
  "scripts": {
    "changeset": "changeset",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run typecheck && eslint .",
    "format": "prettier --write .",
    "format:write": "prettier --ignore-path .gitignore --write",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "pnpm run lint",
    "test": "c8 pnpm run quick:test",
    "types:check": "attw --pack . --profile esm-only --profile node16 --ignore-rules=cjs-resolves-to-esm",
    "prebuild": "pnpm run lint && pnpm run clean",
    "build": "tsc",
    "postbuild": "pnpm run copy:templates",
    "version-packages": "changeset version && pnpm run format:write CHANGELOG.md && git add package.json CHANGELOG.md",
    "release": "pnpm run build && changeset publish",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "atproto",
    "at protocol",
    "oauth"
  ],
  "author": "Emelia Smith",
  "license": "MIT",
  "contributors": [
    {
      "name": "Emelia Smith",
      "url": "https://github.com/thisismissem"
    }
  ],
  "repository": {
    "url": "https://github.com/ThisIsMissEm/adonisjs-atproto-oauth"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/auth": "^9.5.1",
    "@adonisjs/core": "^6.12.0",
    "@adonisjs/eslint-config": "2.1.2",
    "@adonisjs/lucid": "^21.8.2",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@atproto-labs/simple-store": "^0.3.0",
    "@atproto/jwk-jose": "^0.1.11",
    "@atproto/lex": "^0.0.11",
    "@atproto/lex-schema": "^0.0.10",
    "@atproto/oauth-client-node": "^0.3.15",
    "@atproto/syntax": "^0.4.3",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@japa/assert": "^4.2.0",
    "@japa/runner": "^5.0.0",
    "@swc/core": "^1.6.3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.3",
    "@vinejs/vine": "^4.2.0",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^7.0.0",
    "eslint": "^9.15.0",
    "luxon": "^3.7.2",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "@adonisjs/auth": "^9.5.1",
    "@adonisjs/core": "^6.2.0",
    "@adonisjs/lucid": "^21.8.2",
    "@atproto-labs/simple-store": "^0.3.0",
    "@atproto/jwk-jose": "^0.1.11",
    "@atproto/lex": "^0.0.11",
    "@atproto/lex-schema": "^0.0.10",
    "@atproto/oauth-client-node": "^0.3.15",
    "@vinejs/vine": "^4.2.0",
    "luxon": "^3.7.2"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "dependencies": {
    "@poppinss/macroable": "^1.1.0"
  }
}
